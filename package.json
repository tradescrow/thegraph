{
  "name": "tradescrow",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ --version-label=v1.0.3",
    "deploy:avalanche": "yarn deploy --network=avalanche tradescrow-avalanche",
    "deploy:arbitrum": "yarn deploy --network=arbitrum-one tradescrow-arbitrum",
    "deploy:mainnet": "yarn deploy --network=mainnet tradescrow-mainnet",
    "deploy:base": "yarn deploy --network=base tradescrow-base",
    "create-local": "graph create --node http://localhost:8020/ tradescrow",
    "remove-local": "graph remove --node http://localhost:8020/ tradescrow",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 tradescrow",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.51.2",
    "@graphprotocol/graph-ts": "0.30.0"
  },
  "devDependencies": { "matchstick-as": "0.5.0" }
}
